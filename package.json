{
  "name": "my-electron-peek-ai",
  "productName": "my-electron-peek-ai",
  "version": "1.0.0",
  "description": "My Electron application description",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently \"vite --config vite.config.js\" \"wait-on http://localhost:3000 && electron-forge start\"",
    "build": "npm run build:renderer && npm run build:preload",
    "build:renderer": "vite build --config vite.config.js",
    "build:preload": "vite build --config vite.preload.config.js",
    "preview": "vite preview --config vite.config.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "restart": "npm run build:preload && npm run start"
  },
  "keywords": [],
  "author": {
    "name": "daniwidodo",
    "email": "work.daniarthurwidodo@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^5.27.4",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.544.0",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "electron": "38.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "wait-on": "^9.0.1"
  }
}
